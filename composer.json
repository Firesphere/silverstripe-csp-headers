{
    "name": "firesphere/cspheaders",
    "description": "Setup CSP Headers for a website",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CSP",
        "Content",
        "Security",
        "Policies"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Simon 'Firesphere' Erkelens",
            "email": "github@casa-laguna.net"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-simplexml": "*",
        "silverstripe/cms": "^4",
        "paragonie/csp-builder": "v2.8.1",
        "guzzlehttp/guzzle": ">=6"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Firesphere/csp-builder.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "3.*"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
